{
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    },
    "Console": {
      "Default": "Trace"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "server=.;database=ItemsDb;integrated security=true; trust server certificate=true"
  },
  "Attachments": {
    "MaxSize": 100,
    "AllowedTypes": "pdf;doc;docx;xls;xlsx;txt",
    "EnableCompression": true
  },
  "Jwt": {
    "Issuer": "SimpleDotNetWebApiAppBackEnd",
    "Audience": "SimpleDotNetWebApiAppFrontEnd",
    "lifetime": 60,
    "SigningKey": "SimpleDotNetWebApiSimpleDotNetWebApi"
  }
}
